{
  "name": "movie-night",
  "version": "1.0.0",
  "description": "MV* Unit Testing",
  "main": "webpack.config.js",
  "scripts": {
    "dev": "webpack-dev-server --inline --hot --progress --colors",
    "wp": "webpack --progress --colors",
    "build": "webpack -p --define process.env.NODE_ENV='\"production\"' --progress --colors",
    "stats": "webpack --json > stats.json",
    "test": "jest",
    "test-watch": "jest --verbose --watch"
  },
  "jest": {
    "verbose": true,
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/__tests__/"
    ],
    "collectCoverage": true
  },
  "author": "Fredrik, Niklas, Tobias",
  "license": "ISC",
  "devDependencies": {
    "axios": "^0.15.3",
    "axios-mock-adapter": "^1.7.1",
    "babel-core": "^6.21.0",
    "babel-jest": "^18.0.0",
    "babel-loader": "^6.2.10",
    "babel-plugin-transform-object-rest-spread": "^6.22.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.22.0",
    "chai": "^3.5.0",
    "css-loader": "^0.26.1",
    "enzyme": "^2.7.1",
    "immutable": "^3.8.1",
    "jest": "^18.1.0",
    "moment": "^2.17.1",
    "react": "^15.4.2",
    "react-addons-test-utils": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-intl": "^2.2.3",
    "react-redux": "^5.0.1",
    "react-router": "^3.0.0",
    "react-test-renderer": "^15.4.2",
    "redux": "^3.6.0",
    "redux-logger": "^2.7.4",
    "redux-mock-store": "^1.2.2",
    "redux-promise": "^0.5.3",
    "sinon": "^1.17.7",
    "style-loader": "^0.13.1",
    "webpack": "^1.14.0",
    "webpack-dev-server": "^1.16.2"
  }
}
